{
  "name": "yomali/traffic-tracker",
  "description": "Professional website traffic tracking system",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Your Name",
      "email": "your.email@example.com"
    }
  ],
  "require": {
    "php": "^8.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "squizlabs/php_codesniffer": "^3.7",
    "phpstan/phpstan": "^1.10"
  },
  "autoload": {
    "psr-4": {
      "Yomali\\Tracker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Yomali\\Tracker\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
    "cs": "vendor/bin/phpcs --standard=PSR12 src/",
    "cs-fix": "vendor/bin/phpcbf --standard=PSR12 src/",
    "stan": "vendor/bin/phpstan analyse src --level=7"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}